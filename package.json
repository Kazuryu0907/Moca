{
  "name": "moca",
  "productName": "Moca",
  "description": "RL overlay for GracesBlaze ",
  "version": "0.1.0",
  "license": "MIT",
  "author": {
    "name": "kazuryu"
  },
  "build": {
    "extraMetadata": {
      "main": "./dist/main.js"
    },
    "files": [
      "dist/**/*"
    ],
    "win": {
      "icon": "./moca.png"
    },
    "electronDownload": {
      "mirror": "https://npmmirror.com/mirrors/electron/"
    }
  },
  "repository": {
    "url": "https://github.com/Kazuryu0907/Moca.git"
  },
  "main": "dist/main.js",
  "scripts": {
    "dev": "rimraf dist && run-p dev:webpack dev:electron",
    "check": "tsc --noEmit",
    "build": "rimraf dist && cross-env NODE_ENV=\"production\" webpack --progress",
    "fmt": "dprint fmt",
    "dev:webpack": "cross-env NODE_ENV=\"development\" webpack --progress",
    "dev:electron": "wait-on dist/main.js dist/index.html && electronmon  . --trace-warnings",
    "pack": "electron-builder --dir --win --x64 -c.electronVersion=25.9.8",
    "dist": "electron-builder -c.electronVersion",
    "debug": "webpack-dev-server --config webpack.debug.config.js "
  },
  "electronmon": {
    "patterns": [
      "!graphics/**/*",
      "dist/**/*"
    ]
  },
  "dependencies": {
    "@heroicons/react": "^2.1.5",
    "dotenv": "^16.4.5",
    "google-auth-library": "10.0.0",
    "google-spreadsheet": "^4.1.4",
    "googleapis": "^150.0.1",
    "iconv-lite": "^0.6.3",
    "ip": "^2.0.1",
    "qrcode.react": "^4.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^6.28.0",
    "react-router-dom": "^6.28.0",
    "react-step-progress": "^1.0.3",
    "ws": "^8.18.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.5.0",
    "@electron/packager": "^18.3.6",
    "@eslint/js": "^9.14.0",
    "@types/electron-load-devtool": "^1.2.3",
    "@types/ip": "^1.1.3",
    "@types/node": "^20.17.6",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/server-destroy": "^1.0.4",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.11.0",
    "dprint": "^0.50.0",
    "electron": "^25.9.8",
    "electron-builder": "^25.1.8",
    "electron-load-devtool": "^1.2.0",
    "electron-packager": "^17.1.2",
    "electron-squirrel-startup": "^1.0.1",
    "electronmon": "^2.0.3",
    "esbuild-loader": "^4.2.2",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.13.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.12.0",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.15",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0",
    "wait-on": "^8.0.1",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  }
}
