{
  "name": "electron-react-ts",
  "productName": "Moca",
  "description": "electron-react-ts",
  "version": "0.1.0",
  "license": "MIT",
  "author": {
    "name": "kazum"
  },
  "build": {
    "extraMetadata": {
      "main": "./dist/main.js"
    },
    "files": [
      "dist/**/*"
    ],
    "win": {
      "icon": "./moca.png"
    }
  },
  "repository": {
    "url": "https://github.com/kazum/electron-react-ts.git"
  },
  "main": "dist/main.js",
  "scripts": {
    "dev": "rimraf dist && run-p dev:webpack dev:electron",
    "check": "tsc --noEmit",
    "build": "rimraf dist && cross-env NODE_ENV=\"production\" webpack --progress",
    "dev:webpack": "cross-env NODE_ENV=\"development\" webpack --progress",
    "dev:electron": "wait-on dist/main.js dist/index.html && electronmon  . --trace-warnings",
    "pack": "electron-builder --dir --win --x64 -c.electronVersion",
    "dist": "electron-builder -c.electronVersion",
    "debug": "webpack-dev-server --config webpack.debug.config.js ",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "electronmon": {
    "patterns": [
      "!graphics/**/*",
      "dist/**/*"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@google-cloud/local-auth": "^3.0.1",
    "@heroicons/react": "^2.1.5",
    "@hono/node-server": "^1.13.5",
    "@mui/material": "^5.16.7",
    "ajv": "^8.17.1",
    "arrify": "^3.0.0",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "dseg": "^0.46.0",
    "google-auth-library": "^9.14.2",
    "google-spreadsheet": "^4.1.4",
    "googleapis": "^140.0.1",
    "googleapis-common": "^6.0.4",
    "hono": "^4.6.9",
    "iconv-lite": "^0.6.3",
    "ip": "^2.0.1",
    "open": "^9.1.0",
    "qrcode.react": "^4.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^6.27.0",
    "react-router-dom": "^6.27.0",
    "react-step-progress": "^1.0.3",
    "server-destroy": "^1.0.1",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.5.0",
    "@eslint/js": "^9.14.0",
    "@types/electron-load-devtool": "^1.2.3",
    "@types/google-spreadsheet": "^3.3.2",
    "@types/ip": "^1.1.3",
    "@types/node": "^20.17.6",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/server-destroy": "^1.0.4",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.11.0",
    "electron": "^25.9.8",
    "electron-builder": "^25.1.8",
    "electron-load-devtool": "^1.2.0",
    "electron-packager": "^17.1.2",
    "electronmon": "^2.0.3",
    "esbuild-loader": "^3.2.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-react": "^7.37.2",
    "globals": "^15.12.0",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "postcss-loader": "^7.3.4",
    "prettier": "^3.3.3",
    "rimraf": "^5.0.10",
    "tailwindcss": "^3.4.14",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0",
    "wait-on": "^7.2.0",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2"
  }
}
